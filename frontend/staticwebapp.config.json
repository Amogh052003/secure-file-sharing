{
  "routes": [
    {
      "route": "/upload",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/download",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/",
      "serve": "/index.html"
    }
  ],
  "auth": {
    "identityProviders": {
      "github": {
        "registration": {
          "enabled": true
        }
      }
    }
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY"
  }
}
